import numpy as np
roll_num=np.array([1,20])
salary_range=np.linspace(10000,500000,5)
attendance=np.zero=((5,3))
status=np.ones(5)
marks=np.random.randint(40,100,size=(5,3))
print("Roll Number:",roll_num)
print("Salary Range:",salary_range)
print("Attendance:\n",attendance)
print("Status:",status)
print("Marks:\n",marks)
